{
  "env": {
    "commonjs": true,
    "es2020": true,
    "node": false
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "no-console": 0,
    "quotes": ["error", "single"],
    "object-shorthand": 0,
    "no-undef": 0,
    "strict": 1,
    "lines-around-directive": 1,
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "object-curly-newline": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "array-callback-return": 0,
    "consistent-return": 0,
    "arrow-body-style": 0,
    "no-plusplus": 0,
    "import/newline-after-import": 0,
    "import/no-dynamic-require": 0,
    "no-shadow": 0,
    "no-return-await": 0,
    "operator-linebreak": 0,
    "global-require": 0,
    "semi": ["error", "never"]
  }
}
